        "league/plates": "3.1.1",
        "gabordemooij/redbean": "4.2.1",
        "louislam/louislam-utilities": "dev-master",
        "datatables/datatables": "1.10.8",
        "components/jquery": "1.11.3",
        "blueimp/jquery-file-upload": "9.11.0",
        "almasaeed2010/adminlte": "2.3.0",
        "fortawesome/font-awesome": "4.4.0",
        "phpoffice/phpexcel": "1.8.1",
