{
    "name": "louislam/louislam-crud",
    "description": "An easy-to-use CRUD library for you to create a back-end system or RESTful API in a few minutes.",
    "keywords": ["crud", "backend", "restful", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Louis Lam",
            "email": "louis@louislam.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "slim/slim": "2.6.2",
        "league/plates": "3.1.1",
        "gabordemooij/redbean": "4.2.1",
        "ckeditor/ckeditor": "4.5.2",
        "louislam/louislam-utilities": "dev-master",
        "datatables/datatables": "1.10.7",
        "components/jquery": "1.11.3",
        "gargron/fileupload": "1.0.2",
        "blueimp/jquery-file-upload": "9.11.0",
        "almasaeed2010/adminlte": "2.3.0",
        "fortawesome/font-awesome": "4.4.0"
    },
    "autoload": {
        "psr-4": {
            "LouisLam\\" : "src/LouisLam"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "post-update-cmd": "LouisLam\\CRUD\\CleanUp::cleanUp"
    }
}
